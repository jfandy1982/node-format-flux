{
  "name": "@jfandy1982/node-format-flux",
  "version": "0.0.0-SNAPSHOT",
  "license": "MIT",
  "scripts": {
    "cleanup:all": "npm run cleanup:generated && npm run cleanup:deps && npm install",
    "cleanup:cache:nx": "rimraf ./.nx",
    "cleanup:deps": "rimraf ./package-lock.json && rimraf ./node_modules",
    "cleanup:dist": "rimraf ./dist/lib",
    "cleanup:generated": "npm run cleanup:cache:nx && npm run cleanup:dist && rimraf ./coverage",
    "compile": "npm run cleanup:dist && nx run-many -t build --all && nx run-many -t build-es5 --all && rimraf ./tmp",
    "format:affected": "nx format:write",
    "format:all": "nx format:write --all",
    "nx": "nx",
    "upgrade": "nx migrate latest --interactive",
    "upgrade:doit": "npm install && nx migrate --run-migrations --if-exists"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nx/eslint": "20.7.1",
    "@nx/eslint-plugin": "20.7.1",
    "@nx/jest": "20.7.1",
    "@nx/js": "20.7.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.12",
    "@types/node": "18.16.9",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "20.7.1",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "verdaccio": "^6.0.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "nx": {
    "includedScripts": []
  }
}
