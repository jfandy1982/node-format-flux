{
  "exclude": [],
  "jsc": {
    "externalHelpers": true,
    "keepClassNames": true,
    "loose": true,
    "parser": {
      "decorators": true,
      "dynamicImport": true,
      "syntax": "typescript"
    },
    "target": "es2017",
    "transform": {
      "decoratorMetadata": true,
      "legacyDecorator": true
    }
  },
  "module": {
    "type": "es6"
  },
  "sourceMaps": true
}